    comment = {http://arxiv.org/abs/gr-qc/9710117},
    day = {31},
    doi = {10.1103/PhysRevD.59.102001},
    journal = {Physical Review D},
    keywords = {ligo},
    month = mar,
    number = {10},
    pages = {102001+},
    posted-at = {2006-09-15 16:45:25},
    priority = {2},
    publisher = {American Physical Society},
    title = {{Detecting a stochastic background of gravitational radiation: Signal processing strategies and sensitivities}},
    url = {http://dx.doi.org/10.1103/PhysRevD.59.102001},
    volume = {59},
    year = {1999}
}

