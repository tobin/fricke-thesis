@article{DooleyAngular,
    abstract = {We present the design and performance of the LIGO Angular Sensing 
and Control (ASC) subsystem as implemented for the sixth science run 
of the LIGO interferometers. A statically unstable radiation 
pressure torque induced opto-mechanical mode limited the operation 
of the Initial LIGO interferometers to 7\~{}W input power. We designed 
and implemented an improved ASC control scheme for Enhanced LIGO, an 
incremental upgrade to the Initial LIGO interferometers, designed to 
run with 30\~{}W input power. Through a change of basis, the new ASC 
design directly addresses the statically unstable mode, allowing for 
interferometer operation at powers greater than 7\~{}W and good noise 
performance. Highlights of the ASC performance include: successful 
suppression of unstable modes, limited impact on in-band strain 
sensitivity, and rms mirror motions of \$\le 1\times 10^{-7}\$ 
rad. The demonstrated success of the Enhanced LIGO ASC model and 
design provides confidence in the ASC design for Advanced LIGO.},
    author = {Dooley, Kate and Barsotti, Lisa and Evans, Matt},
    citeulike-article-id = {9595304},
    citeulike-linkout-0 = {https://dcc.ligo.org/cgi-bin/private/DocDB/ShowDocument?docid=67987},
    howpublished = {in preparation},
    journal = {in preparation},
    posted-at = {2011-09-05 06:01:58},
    priority = {2},
    title = {{Angular Sensing and Control of the Enhanced LIGO Interferometers}},
    url = {https://dcc.ligo.org/cgi-bin/private/DocDB/ShowDocument?docid=67987}
}

@article{Dooley2011Characterization,
    abstract = {{We present the design and performance of the LIGO Input Optics subsystem 
as implemented for the sixth science run of the LIGO interferometers. The 
Initial LIGO Input Optics experienced thermal side effects when operating 
with 7 W input power. We designed, built, and implemented improved versions 
of the Input Optics for Enhanced LIGO, an incremental upgrade to the Initial 
LIGO interferometers, designed to run with 30 W input power. At 4 times 
the power of Initial LIGO, the Enhanced LIGO Input Optics demonstrated 
improved performance including: optical efficiency of 75\%, isolation of 26 dB, 
thermal drift of 3 µrad/W, mode matching of 92\%, and negligible thermal 
lensing. The success of the Input Optics design boosts conﬁdence for its ability 
to perform well in Advanced LIGO.}},
    author = {Dooley, Katherine and Arain, Muzammil and Feldbaum, David and Frolov, Valery and Heintze, Matthew and Hoak, Daniel and Khazanov, Eﬁm and Lucianetti, Antonio and Martin, Rodica and M\"{u}ller, Guido and Palashov, Oleg and Quetschke, Volker and Reitze, David H. and Savage, Rick and Tanner, David and Williams, Luke and Wu, Wan},
