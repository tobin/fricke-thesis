@techreport{Smith2011Optimal,
    abstract = {{Critically coupled resonant optical cavities are often used as mode cleaners in optical systems. In optical
systems where some physical quantity is encoded as an amplitude modulation of a laser beam, a mode
cleaner may be used to improve the signal to noise ratio (SNR) of the sensing of said physical quantity.
An automatic alignment system which is primarily sensitive to the carrier ﬁeld component of the beam will
not in general provide the maximum detected SNR. We present two additional approaches which modify
traditional dither alignment sensing, both utilizing large amplitude injected signal ﬁelds which can be used
to produce error signals which are sensitive to the signal ﬁelds. When used in conjunction with alignment
actuators, these approaches can improve the detected SNR. We also present a generalization of this type of a
approach to any time of alignment sensor. Finally, we show the LIGO H1 Interferometer as an example system
where one of these approaches improved the detected SNR by a factor of 3.}},
    author = {Smith, Nicolas and Mavalvala, Nergis},
    citeulike-article-id = {9156238},
    day = {12},
    institution = {Massachusetts Institute of Technology},
    month = apr,
    posted-at = {2011-04-15 00:08:53},
    priority = {2},
    title = {{Optimal Alignment Sensing of a Mode Cleaner Cavity used for Optical
Readout}},
    year = {2011}
}

