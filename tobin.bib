    journal = {J Struct Biol},
    keywords = {tobinfricke},
    number = {1-2},
    pages = {209--218},
    pmid = {14643223},
    posted-at = {2006-09-15 16:46:38},
    priority = {2},
    title = {{Three-dimensional diffractive imaging for crystalline monolayers with one-dimensional compact support.}},
